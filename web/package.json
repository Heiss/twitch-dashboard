{
  "name": "twitch-web",
  "version": "0.1.0",
  "description": "A web dashboard for twitch",
  "scripts": {
    "build": "webpack --config webpack.config.js",
    "watch": "webpack --watch --progress --config webpack.config.js",
    "start": "webpack serve --inline --hot --no-info --config webpack.config.js",
    "clean": "rm -rf dist"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Heiss/twitch-dashboard.git"
  },
  "keywords": [
    "rust",
    "webpack",
    "crankjs"
  ],
  "source": "src/index.jsx",
  "author": "Peter Heiss <peter.heiss@uni-muenster.de>",
  "license": "(MIT)",
  "bugs": {
    "url": "https://github.com/Heiss/twitch-dashboard/issues"
  },
  "homepage": "https://github.com/Heiss/twitch-dashboard#readme",
  "eslint": {
    "settings": {
      "react": {
        "pragma": "createElement"
      }
    }
  },
  "dependencies": {
    "@bikeshaving/crank": "^0.3.7",
    "@fortawesome/fontawesome-free": "^5.15.1",
    "chart.js": "^2.9.4",
    "css-loader": "^5.0.1",
    "jquery": "^3.5.1",
    "path": "^0.12.7",
    "regenerator-runtime": "^0.13.7",
    "style-loader": "^2.0.0"
  },
  "devDependencies": {
    "@babel/core": "^7.12.9",
    "@babel/preset-env": "^7.12.11",
    "@babel/preset-react": "^7.9.4",
    "@webpack-cli/serve": "^1.1.0",
    "autoprefixer": "^10.0.4",
    "babel-loader": "^8.2.2",
    "babel-preset-crank": "^0.2.0",
    "clean-webpack-plugin": "^3.0.0",
    "copy-webpack-plugin": "^5.0.0",
    "html-webpack-plugin": "^4.5.0",
    "mini-css-extract-plugin": "^1.3.3",
    "postcss": "^8.1.14",
    "postcss-cli": "^8.3.1",
    "postcss-loader": "^4.1.0",
    "postcss-preset-env": "^6.7.0",
    "tailwindcss": "^2.0.2",
    "webpack": "^4.44.2",
    "webpack-cli": "^3.3.12",
    "webpack-dev-server": "^3.11.0"
  }
